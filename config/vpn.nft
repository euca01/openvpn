table ip nat {
	chain postrouting {
		type nat hook postrouting priority 100; policy accept;
		ip saddr 172.100.0.0/16 oif "tun0" snat to 10.8.0.4

	}

	chain prerouting {
		type nat hook prerouting priority -100; policy accept;
        ip daddr 10.8.0.4 iif "tun0" tcp dport 25 dnat to 172.100.0.10
	}
}

